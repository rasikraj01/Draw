{
  "manifest_version": 2,

  "name": "Draw",
  "description": "A extension  which helps users to draw on webpages.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/img/icon.png"
      },
  "permissions": [
    "activeTab",
    "storage"
],
"content_scripts": [{
   "matches": ["<all_urls>"],
    "js": ["assets/js/p5.min.js","assets/js/p5.dom.min.js", "assets/js/draw.js"]
  }]
}
